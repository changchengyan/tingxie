<import src="../template/template.wxml" />
<view class="contain">
<view class="top">
  <view class="title">{{name}}</view>
  <view class="star">
    <view class="  unit-star-{{star}}">
      <view></view>
      <view style="margin-top:-40rpx"></view>
      <view></view>
    </view>
  </view>
</view>
<view class="tips">
	<view wx:if="{{star>1}}">恭喜你！完成听写，获得了{{star}}颗星星。</view>
	<view wx:elif="{{star==1}}">本次听写获得了{{star}}颗星星，继续加油哦！</view>
	<view wx:elif="{{star==0}}">别灰心，多加练习，再接再厉！</view>
</view>
<view class="end">
  <view class="me">
    <image src="../../images/finally/list.png" bindtap="returnList"></image>
    <image src="../../images/finally/Share-to-Facebook.png" style="width:84rpx;height:84rpx" bindtap="playAgain"></image>
    <image src="../../images/finally/next.png" bindtap="nextPlay"></image>
  </view>
  <view class="end_in">
  
  </view>
</view>
</view>